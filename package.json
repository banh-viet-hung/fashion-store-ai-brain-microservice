{
    "name": "fashion-store-ai-brain-microservice",
    "version": "1.0.0",
    "description": "RAG-based AI microservice with LangChain and LangGraph",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "build": "tsc",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
    },
    "dependencies": {
        "@langchain/community": "latest",
        "@langchain/google-genai": "latest",
        "@langchain/langgraph": "latest",
        "cheerio": "latest",
        "dotenv": "latest",
        "langchain": "latest",
        "zod": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "nodemon": "latest",
        "ts-node": "latest",
        "typescript": "latest"
    }
}